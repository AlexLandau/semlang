
@Test(["1"], "1")
@Test(["2"], "1")
@Test(["3"], "2")
@Test(["42"], "3")
@Test(["0"], "0")
function getBitsSumOfNatural(n: Natural): Natural {
  let bits: BitsBigEndian = Natural.toBits(n)
  List.reduce<Bit, Natural>(bits->bits, Natural."0", function (prevSum: Natural, bit: Bit) {
    Natural.plus(prevSum, bit->natural)
  })
}
