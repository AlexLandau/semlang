
@Test(["1"], "1")
@Test(["2"], "1")
@Test(["3"], "2")
@Test(["42"], "3")
@Test(["0"], "0")
function getBitsSumOfNatural2(n: Natural2): Natural2 {
  let bits: BitsBigEndian = Natural2.toBits(n)
  List.reduce<Bit, Natural2>(bits->bits, Natural2."0", function (prevSum: Natural2, bit: Bit) {
    Natural2.plus(prevSum, bit->natural)
  })
}
