

function mvpPythagoreanTripleCheck(a: natural, b: natural, c: natural): boolean {
  let aSquared: natural = times(a, a)
  let bSquared: natural = times(b, b)
  let cSquared: natural = times(c, c)
  let sumOfSquares: natural = plus(aSquared, bSquared)
  let result: boolean = sumOfSquares == cSquared
  return result
}
