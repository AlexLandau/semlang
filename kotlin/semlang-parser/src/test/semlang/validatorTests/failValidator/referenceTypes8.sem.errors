// Reference types cannot be aliased via if/then statements
function duplicateAnAlias(out1: &TextOut, out2: &TextOut, useTwo: Boolean): Void {
  let out3 = if (useTwo) {
      ~~~~ We are assigning a reference to the variable out3, but it may already have an alias; references are not allowed to have more than one alias
    out2
  } else {
    out1
  }
  TextOut.print(out1, Unicode.String."Larry")
  TextOut.print(out2, Unicode.String."Curly")
  TextOut.print(out3, Unicode.String."Moe")
}
