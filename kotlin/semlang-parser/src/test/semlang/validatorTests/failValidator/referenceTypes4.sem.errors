// Reference types cannot be called multiple times in one statement
function doSomething(out: &TextOut): Void {
  doubleSayHi(out, out)
                   ~~~ Variable out is a reference and cannot be used more than once in a statement
}

function doubleSayHi(out1: &TextOut, out2: &TextOut): Void {
  TextOut.print(out1, Unicode.String."Hi!\n")
  TextOut.print(out2, Unicode.String."Hi!\n")
}
