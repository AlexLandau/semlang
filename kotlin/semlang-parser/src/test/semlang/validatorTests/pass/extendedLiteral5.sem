struct Digit {
  natural: Natural2
  requires {
    Integer.lessThan(natural->integer, Integer."10")
  }
}

struct EvenDigit {
  digit: Digit
  requires {
    Integer.equals(Integer."0", Try.assume<Integer>(Integer.modulo(digit->natural->integer, Integer."2")))
  }
}

function getEvenDigit(): EvenDigit {
  EvenDigit."6"
}
