
function euler.problem1(): Natural {
  let range: List<Natural> = Natural.rangeInclusive(Natural."1", Natural."999")
  let multiples: List<Natural> = List.filter<Natural>(range, euler.multiplesFilter|(_))
  Natural.sum(multiples)
}

function euler.multiplesFilter(n: Natural): Boolean {
  let multipleOfThree: Boolean = Natural.equals(Natural."0", Try.assume<Natural>(Natural.modulo(n, Natural."3")))
  let multipleOfFive: Boolean = Natural.equals(Natural."0", Try.assume<Natural>(Natural.modulo(n, Natural."5")))
  Boolean.or(multipleOfThree, multipleOfFive)
}
