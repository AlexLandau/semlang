{
    "name": "semlang-code",
    "displayName": "semlang-code",
    "description": "foo",
    "version": "0.0.1",
    "publisher": "donthaveoneyet",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:semlang"
    ],
    "main": "./out/main",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "contributes": {
        "languages": [
            {
                "id": "semlang",
                "aliases": [
                    "Semlang",
                    "semlang"
                ],
                "extensions": [
                    ".sem",
                    ".sem1",
                    ".sem0"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "semlang",
                "scopeName": "source.semlang",
                "path": "./syntaxes/semlang.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "vscode": "^1.1.5",
        "vscode-languageclient": "^3.4.2"
    },
    "devDependencies": {
        "@types/node": "^7.0.43",
        "typescript": "~2.5.3"
    }
}
