@Test(["123456781234567812345678", "876543218765432187654321", "999999999999999999999999"], "true")
@Test(["123456781234567812345678", "876543218765432187654321", "999999999999999999999998"], "false")
@Test(["123456781234567812345678", "876543218765432187654321", "1000000000000000000000000"], "false")
function testIsSum(left: Integer, right: Integer, possibleSum: Integer): Boolean {
  Integer.equals(Integer.plus(left, right), possibleSum)
}
