function getFirstCharacter(str: Unicode.String): Try<Natural> {
    Try.map<Unicode.CodePoint, Natural>(List.get<Unicode.CodePoint>(str->value, Natural."0"), getValue|(_))
}

function getValue(cp: Unicode.CodePoint): Natural {
    cp->value
}
