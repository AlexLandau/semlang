function getFirstCharacter(str: String): Maybe<Natural> {
    Maybe.map<CodePoint, Natural>(List.get<CodePoint>(str->codePoints, Natural."0"), getValue|(_))
}

function getValue(cp: CodePoint): Natural {
    cp->natural
}
