function getFirstCharacter(str: Unicode.String): Try<Natural2> {
    Try.map<Unicode.CodePoint, Natural2>(List.get<Unicode.CodePoint>(str->codePoints, Natural2."0"), getValue|(_))
}

function getValue(cp: Unicode.CodePoint): Natural2 {
    cp->natural
}
