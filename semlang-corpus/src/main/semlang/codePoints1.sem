@Test(["0"], "success(0)")
@Test(["70"], "success(70)")
@Test(["1114111"], "success(1114111)")
@Test(["1114112"], "failure")
function createCodePoint(value: Natural2): Try<Natural2> {
  let codePointMaybe = Unicode.CodePoint(value)
  Try.map<Unicode.CodePoint, Natural2>(codePointMaybe, getCodePointValue|(_))
}

function getCodePointValue(codePoint: Unicode.CodePoint): Natural2 {
  codePoint->natural
}
